<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="2020030900" id="com.raulalvarez.munfordapp" version="1.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>El Caso Munford</name>
    <description>
		Aplicación para la reproducción de audios de la exposición de El Caso Munford
	</description>
    <author email="me@raul-alvarez.es" href="http://raul-alvarez.es">
		Raul Alvarez
	</author>
    <content src="index.html" />
    <icon src="www/faviconMunford.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="AndroidLaunchMode" value="singleInstance" />
    <preference name="KeepRunning" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name="orientation" value="portrait" />
</widget>
